<mvc:View controllerName="masterdatamanagement.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">

    <content>
            <IconTabBar id="mainIconTabBar">
                <items>
                    <!-- Categories Tab -->
                    <IconTabFilter id="_IDGenIconTabFilter1" text="Categories" key="Categories">
                        <VBox id="_IDGenVBox">
                            <Toolbar id="_IDGenToolbar">
                                <Title id="_IDGenTitle" text="Categories" level="H2" />
                                <ToolbarSpacer id="_IDGenToolbarSpacer" />
                                <Button id="_IDGenButton" icon="sap-icon://add" tooltip="Add Category" press="onAddCategory" />
                            </Toolbar>
                            <Table id="categoriesTable" inset="false" items="{/Categories}">
                                <columns>
                                    <Column id="_IDGenColumn">
                                        <Text id="_IDGenText" text="ID" />
                                    </Column>
                                    <Column id="_IDGenColumn1">
                                        <Text id="_IDGenText1" text="Name" />
                                    </Column>
                                    <Column id="_IDGenColumn2">
                                        <Text id="_IDGenText2" text="Description" />
                                    </Column>
                                    <Column id="_IDGenColumn3" hAlign="Center" width="5em">
                                        <Text id="_IDGenText3" text="Actions" />
                                    </Column>
                                </columns>
                                <items>
                                    <ColumnListItem id="_IDGenColumnListItem">
                                        <cells>
                                            <Text id="_IDGenText4" text="{ID}" />
                                            <Text id="_IDGenText5" text="{Name}" />
                                            <Text id="_IDGenText6" text="{Description}" />
                                            <HBox id="_IDGenHBox">
                                                <Button id="_IDGenButton1" icon="sap-icon://edit" tooltip="Edit" press="onEditCategory" type="Transparent" />
                                                <Button id="_IDGenButton2" icon="sap-icon://delete" tooltip="Delete" press="onDeleteCategory" type="Transparent" />
                                            </HBox>
                                        </cells>
                                    </ColumnListItem>
                                </items>
                            </Table>
                        </VBox>
                    </IconTabFilter>

                    <!-- Financing Types Tab -->
                    <IconTabFilter id="_IDGenIconTabFilter2" text="Financing Types" key="FinancingTypes">
                        <VBox id="_IDGenVBox1">
                            <Toolbar id="_IDGenToolbar1">
                                <Title id="_IDGenTitle1" text="Financing Types" level="H2" />
                                <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                                <Button id="_IDGenButton3" icon="sap-icon://add" tooltip="Add Financing Type" press="onAddFinancingType" />
                            </Toolbar>
                            <Table id="financingTypesTable" inset="false" items="{/FinancingTypes}">
                                <columns>
                                    <Column id="_IDGenColumn4">
                                        <Text id="_IDGenText7" text="ID" />
                                    </Column>
                                    <Column id="_IDGenColumn5">
                                        <Text id="_IDGenText8" text="Name" />
                                    </Column>
                                    <Column id="_IDGenColumn6">
                                        <Text id="_IDGenText9" text="Description" />
                                    </Column>
                                    <Column id="_IDGenColumn7" hAlign="Center" width="5em">
                                        <Text id="_IDGenText10" text="Actions" />
                                    </Column>
                                </columns>
                                <items>
                                    <ColumnListItem id="_IDGenColumnListItem1">
                                        <cells>
                                            <Text id="_IDGenText11" text="{ID}" />
                                            <Text id="_IDGenText12" text="{Name}" />
                                            <Text id="_IDGenText13" text="{Description}" />
                                            <HBox id="_IDGenHBox1">
                                                <Button id="_IDGenButton4" icon="sap-icon://edit" tooltip="Edit" press="onEditFinancingType" type="Transparent" />
                                                <Button id="_IDGenButton5" icon="sap-icon://delete" tooltip="Delete" press="onDeleteFinancingType" type="Transparent" />
                                            </HBox>
                                        </cells>
                                    </ColumnListItem>
                                </items>
                            </Table>
                        </VBox>
                    </IconTabFilter>
</items>
            </IconTabBar>
        </content>
      
    
    
    </Page>
</mvc:View>